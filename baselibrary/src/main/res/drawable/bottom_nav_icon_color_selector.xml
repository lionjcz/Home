<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--    已選擇的顏色-->
    <item android:state_checked="true" android:color="@color/bottomSelected" />
    <!--    按下去的顏色-->
    <item android:state_pressed="true" android:state_enabled="true" android:color="@color/bottonClicking" />
    <!--    沒選擇的顏色-->
    <item android:color="@color/bottomUnSelected" />
</selector>